package dateTest;

import java.util.Date;

/**
 * @author liupuyan
 * Date 特定的瞬间，精确到毫秒
 */
public class demo1 {
	public static void main(String[] args) {
		// 根据当前的默认毫秒值创建日期对象
		Date d1 = new Date();
		System.out.println(d1);
		
		// 根据给定的毫秒值创建日期对象
		long time = System.currentTimeMillis();
		Date d2 = new Date(time);
		System.out.println(d2);
	}
}
