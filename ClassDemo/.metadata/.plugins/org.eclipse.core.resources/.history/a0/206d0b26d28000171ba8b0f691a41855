package ioTest;

import java.io.InputStream;
import java.io.SequenceInputStream;

/**
 * @author liupuyan
 * 合并流
 * 把多个输入流的数据写到一个输出流中
 */
public class SequenceInputStreamDemo1 {
	public static void main(String[] args) {
		InputStream s1;
		InputStream s2;
		SequenceInputStream sis = new SequenceInputStream(s1, s2);
	}
}
