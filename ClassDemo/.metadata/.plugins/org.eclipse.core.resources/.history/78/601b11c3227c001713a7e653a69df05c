package ioTest;

/**
 * @author liupuyan
 * 计算机中 中文的存储分两个字节
 * 	第一个字节肯定是负数
 * 第二个字节常见是负数，也可能为正数
 */
public class demo6 {
	public static void main(String[] args) {
		String str1 = "abcdef";
		byte[] by = str1.getBytes();
		for(byte b: by) {
			System.out.print(b + "\t");
		}
		
		String str2 = "哈喽 你好";
		byte[] by2 = str2.getBytes();
		for(byte b: by2) {
			System.out.print(b + "\t");
		}
	}
}
