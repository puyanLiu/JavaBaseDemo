package mapTest;

import java.util.HashMap;
import java.util.Map;

/**
 * @author liupuyan
 * Map集合
 * 	将键映射到值的对象。一个映射不能包含重复的键，每个键最多只能映射到一个值
 */
public class demo1 {
	public static void main(String[] args) {
		Map<String, String> map = new HashMap<>();
		System.out.println(map.put("id", "1001"));
	}
}
