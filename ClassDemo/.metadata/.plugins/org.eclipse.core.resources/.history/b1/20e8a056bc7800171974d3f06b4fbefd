package vectorTest;

import java.util.Iterator;
import java.util.Vector;

/**
 * @author liupuyan
 * Vector可以被ArrayList取代
 * 取代原因
 * - 安全
 * - 效率
 * - 简化书写
 */
public class demo1 {
	public static void main(String[] args) {
		Vector v1 = new Vector();
		v1.add("hello");
		v1.addElement("world");
		
		Iterator it1 = v1.iterator();
		while (it1.hasNext()) {
			String string = it1.next();
			System.out.println(string);
		}
	
	}
}
