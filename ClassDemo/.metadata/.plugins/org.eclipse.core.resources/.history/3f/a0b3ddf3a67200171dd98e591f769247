package stringTest;

public class demo1 {
	public static void main(String[] args) {
		String s1 = new String();
		System.out.println(s1 + " " + s1.length());
		
		// 字节转数组
		byte[] b = {97, 98, 99, 100, 101};
		String s2 = new String(b);
		System.out.println(s2 + " " + s2.length());
		
		// 把字节数组的一部分转成字符串
		String s3 = new String(b, 1, 3);
		
	}
}
