package stringTest;

/**
 * @author liupuyan
 * 统计一个字符串中大写字母字符，小写字母字符，数组字符出现的次数
 */
public class demo2 {
	
	public static void main(String[] args) {
		String string = "Hello123World";
		int bigCharCount = 0;
		int smallCharCount = 0;
		int numCount = 0;
		for(int i = 0; i < string.length(); i++) {
			if (string.charAt(i) >= 'a' && string.charAt(i) <= 'z') {
				bigCharCount++;
			}
		}
	}
}
