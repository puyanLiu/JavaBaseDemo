package ioTest;

import java.io.IOException;
import java.io.Reader;

public class MyBufferedReader {
	private Reader r;

	public MyBufferedReader(Reader in) {
		this.r = in;
	}

	public String readLine() throws IOException {
		int b = 0;
		while ((b = r.read()) != -1) {
			
		}
		return null;
	}

	public void close() throws IOException {
		r.close();
	}
}
