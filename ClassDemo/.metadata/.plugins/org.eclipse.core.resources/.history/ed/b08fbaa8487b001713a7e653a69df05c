		Student s14 = new Student("赵六", 19);
		a11.add(s11);
		a11.add(s12);
		a11.add(s13);
		a11.add(s14);
		

		ArrayList<Student> a11 = new ArrayList<>();
		Student s11 = new Student("张三", 19);
		Student s12 = new Student("李四", 19);
		Student s13 = new Student("王五", 19);
		Student s14 = new Student("赵六", 19);
		a11.add(s11);
		a11.add(s12);
		a11.add(s13);
		a11.add(s14);
		
		HashMap<String, ArrayList<Student>> hm1 = new HashMap<>();
