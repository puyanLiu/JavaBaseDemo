	public static void main(String[] args) {
		HashMap<String, HashMap<String, ArrayList<Student>>> hm = new HashMap<>();
		ArrayList<Student> a11 = new ArrayList<>();
		Student s11 = new Student("张三", 19);
		Student s12 = new Student("李四", 19);
