package com.animal;

public class Cat extends Animal {
	private string name;
	public string getName() {
		return name;
	}

	public void setName(string name) {
		this.name = name;
	}

	@Override
	public void eat() {
		// TODO Auto-generated method stub
		System.out.println("猫吃鱼");
	}

	@Override
	public void sleep() {
		// TODO Auto-generated method stub
		System.out.println("猫睡觉");
	}
	
}
