package forTest;

import java.util.ArrayList;

/**
 * @author liupuyan
 * 增强for
 * for(元素数据类型 变量:数组或者集合) {
 * 	变量就是元素
 * }
 * 
 * 注意：增强for的目标不能为null
 */
public class demo0 {
	public static void main(String[] args) {
		int[] arr = {1, 2, 3, 4, 5};
		for(int a: arr) {
			System.out.println(a);
		}
		
		String[] strArr = {"张三", "李四", "王五", "赵六"};
		for(String s: strArr) {
			System.out.println(s);
		}
		
		ArrayList<String> al1 = new ArrayList<>();
		al1.add("hello");
		al1.add("world");
		al1.add("java");
	}
}
