package recursiveTest;

import java.util.Arrays;

/**
 * @author liupuyan
 * 
 * 1, 1, 2, 3, 5, 8...
 */
public class demo2 {
	public static void main(String[] args) {
		method1();
	}

	private static void method1() {
		int num = 20;
		int sum = 0;
		int[] arr = new int[20];
		for (int i = 0; i < arr.length; i++) {
			if (i == 0 || i == 1) {
				arr[i] = 1;
			} else {
				arr[i] = arr[i - 2] + arr[i - 1];
			}
		}
		System.out.println(Arrays.toString(arr));
	}
}
