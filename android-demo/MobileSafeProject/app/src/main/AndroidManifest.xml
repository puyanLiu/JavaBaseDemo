<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.queqianme.mobilesafe.app">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/queqianme_logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppNoTitle">
        <activity android:name="com.queqianme.www.mobilesafeproject.activity.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name="com.queqianme.www.mobilesafeproject.activity.HomeActivity" />
        <activity android:name="com.queqianme.www.mobilesafeproject.activity.SettingActivity" />
        <activity android:name="com.queqianme.www.mobilesafeproject.activity.LostAndFindActivity" />
        <activity android:name="com.queqianme.www.mobilesafeproject.activity.Setup1Activity" />
        <activity android:name="com.queqianme.www.mobilesafeproject.activity.Setup2Activity" />
        <activity android:name="com.queqianme.www.mobilesafeproject.activity.Setup3Activity" />
        <activity android:name="com.queqianme.www.mobilesafeproject.activity.Setup4Activity" />
    </application>


</manifest>